<div class = 'row'>
  <div class = 'col-md-9'>
    <table class="table table-hover table-condensed">
      <thead>
        <td class="seq-date">MRCA</td>
        <td class="sequence">{{amino-acid mrcaSlice}}</td>
      </thead>
      <tbody>
        {{#each groupedSequences}}
        <tr>
          <td class="seq-date">{{date}}</td>
          <td class="sequence">
            <ul class="list-unstyled">
              {{#each seq in sequences}}
              <li>{{amino-acid seq}}</li>
              {{/each}}
            </ul>
          </td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
